<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/BlueLight"
    tools:context=".TourDetails" >

    <TextView
        android:id="@+id/textView1"
        android:textColor="#000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PATH NAME"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/tourName"
        android:textColor="#000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:ems="10"
        android:layout_toLeftOf="@+id/imvTour"
        android:layout_toStartOf="@+id/imvTour">

        <requestFocus />
    </EditText>

    <TextView
        android:id="@+id/descr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#000"
        android:text="DESCRIPTION"
        android:textSize="14sp"
        android:layout_below="@+id/imvTour"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/tourDescription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#000"
        android:layout_alignLeft="@+id/descr"
        android:layout_below="@+id/descr"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/textView3"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PATH TYPE"
        android:textSize="14sp"
        android:textColor="#000"
        android:layout_below="@+id/tourDescription"
        android:layout_marginTop="40dp" />

    <Spinner
        android:id="@+id/tourType"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:entries="@array/path_type"
        android:prompt="@string/typePrompt" />

    <Button
        android:id="@+id/button1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="ok"
        android:text="OK"
        android:background="@drawable/nice_button"
        android:textColor="#fff"
        android:layout_above="@+id/button2" />

    <Button
        android:id="@+id/saveButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="save"
        android:background="@drawable/nice_button"
        android:textColor="#fff"
        android:enabled="false"
        android:text="Save"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="cancel"
        android:background="@drawable/nice_button"
        android:textColor="#fff"
        android:text="Cancel"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:gravity="center"
        android:layout_above="@+id/saveButton" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PRICE: $"
        android:textColor="#000"
        android:textSize="14sp"
        android:layout_below="@+id/tourDescription"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:textColor="#000"
        android:textSize="14sp"
        android:ems="10"
        android:text="0"
        android:id="@+id/price"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toLeftOf="@+id/imvTour"
        android:layout_toStartOf="@+id/imvTour" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/imvTour"
        android:onClick="addTourImage"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/descr"
        android:layout_alignEnd="@+id/descr" />


