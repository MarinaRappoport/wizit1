        </activity>
        <activity
            android:name="com.with.tourbuild.SignUp"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.with.tourbuild.Calendar"
            android:screenOrientation="portrait"
            android:theme="@style/CalendTheme" >
        </activity>
        <activity
            android:name="com.with.tourbuild.Chat"
            android:screenOrientation="portrait" >
